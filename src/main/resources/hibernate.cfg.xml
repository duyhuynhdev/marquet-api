        <mapping resource="data/WatchListEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.WatchListEntity"/>-->
        <mapping resource="data/StateEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.StateEntity"/>-->
        <mapping resource="data/AccountEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.ActivityEntity"/>-->
        <mapping resource="data/ActivityEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.AccountEntity"/>-->
        <!-- JDBC connection pool (use the built-in) -->

    </session-factory>
</hibernate-configuration>