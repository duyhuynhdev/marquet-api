<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>

        <property name="connection.url">jdbc:mysql://localhost:3306/marqet</property>
        <property name="connection.username">root</property>
        <property name="connection.password"></property>
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="show_sql">true</property>
        <property name="format_sql">true</property>
        <property name="hibernate.connection.CharSet">utf8</property>
        <property name="hibernate.connection.characterEncoding">utf8</property>
        <property name="hibernate.connection.useUnicode">true</property>
        <property name="hbm2ddl.auto">update</property>
        <mapping resource="data/BigBannerEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.BigBannerEntity"/>-->
        <mapping resource="data/CategoryEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.CategoryEntity"/>-->
        <mapping resource="data/CityEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.CityEntity"/>-->
        <mapping resource="data/CommentEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.CommentEntity"/>-->
        <mapping resource="data/CountryEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.CountryEntity"/>-->
        <mapping resource="data/ElementEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.ElementEntity"/>-->
        <mapping resource="data/FeedbackEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.FeedbackEntity"/>-->
        <mapping resource="data/FollowEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.FollowEntity"/>-->
        <mapping resource="data/InformationEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.InformationEntity"/>-->
        <mapping resource="data/MessageEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.MessageEntity"/>-->
        <mapping resource="data/ProductEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.ProductEntity"/>-->
        <mapping resource="data/SmallBannerEntity.hbm.xml"/>
        <!--<mapping class="com.marqet.WebServer.pojo.SmallBannerEntity"/>-->
        <mapping resource="data/SubCategoryEntity.hbm.xml"/>
