<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.marqet.WebServer.pojo.ActivityEntity" table="Activity" schema="" catalog="marqet">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="date">
            <column name="date" sql-type="bigint" not-null="true"/>
        </property>
        <property name="action">
            <column name="action" sql-type="int" not-null="true"/>
        </property>
        <property name="ref">
            <column name="ref" sql-type="int" not-null="true"/>
        </property>
        <property name="subjectEmail">
            <column name="subjectEmail" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="isRead">
            <column name="isRead" sql-type="int" not-null="true"/>
        </property>
        <property name="objectEmail">
            <column name="objectEmail" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="ownerEmail">
            <column name="ownerEmail" sql-type="varchar" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>