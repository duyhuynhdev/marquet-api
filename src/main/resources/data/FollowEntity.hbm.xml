<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.marqet.WebServer.pojo.FollowEntity" table="Follow" schema="" catalog="marqet">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="follower">
            <column name="follower" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="beFollower">
            <column name="beFollower" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>