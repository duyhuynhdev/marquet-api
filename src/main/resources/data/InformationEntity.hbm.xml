<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.marqet.WebServer.pojo.InformationEntity" table="Information" schema="" catalog="marqet">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="communityRule">
            <column name="communityRule" sql-type="text" length="65535" not-null="true"/>
        </property>
        <property name="pointSystem">
            <column name="pointSystem" sql-type="text" length="65535" not-null="true"/>
        </property>
        <property name="emailSupport">
            <column name="emailSupport" sql-type="text" length="65535" not-null="true"/>
        </property>
        <property name="about">
            <column name="about" sql-type="text" length="65535" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>