<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.marqet.WebServer.pojo.ProductEntity" table="Product" schema="" catalog="marqet">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="bigint" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="22" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="text" length="65535" not-null="true"/>
        </property>
        <property name="productImages">
            <column name="productImages" sql-type="text" length="65535" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="cityCode">
            <column name="cityCode" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="subCategoryId">
            <column name="subCategoryId" sql-type="bigint" not-null="true"/>
        </property>
        <property name="productVideo">
            <column name="productVideo" sql-type="text" length="65535" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>