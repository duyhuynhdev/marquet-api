<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.marqet.WebServer.pojo.AccountEntity" table="Account" schema="" catalog="marqet">
        <id name="email">
            <column name="email" sql-type="varchar" length="100" not-null="true"/>
        </id>
        <property name="userName">
            <column name="userName" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="role">
            <column name="role" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>